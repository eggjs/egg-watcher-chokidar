{
  "name": "egg-watcher-vagrant",
  "version": "1.0.0",
  "description": "enhance egg-watcher with vagrant support",
  "eggPlugin": {
    "name": "watcherVagrant",
    "dep": [
      "watcher"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-watcher"
  ],
  "dependencies": {
    "chokidar": "^1.6.1",
    "debug": "^2.6.1",
    "sdk-base": "^3.0.1"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^0.7.0",
